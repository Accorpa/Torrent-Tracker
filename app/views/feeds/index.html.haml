%h1 Listing feeds
= link_to_function "Toggle panel", "panel.toggle()"

#panel
#entries_container
  %table
    %tr
      %th
      %th
      %th
      %th
      %th

    - @feeds.each do |feed|
      %tr
        %td= feed.name
        %td= link_to 'Edit', edit_feed_path(feed)
        %td= link_to 'Destroy', feed, :confirm => 'Are you sure?', :method => :delete

  %br

= link_to 'New Feed', new_feed_path
= link_to 'Trackings', trackings_path
= link_to 'Torrents', torrents_path
